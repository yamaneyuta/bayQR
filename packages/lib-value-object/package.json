{
  "name": "@workspace/lib-value-object",
  "version": "0.0.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "run-p build:*",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "coverage": "bash ${PROJECT_ROOT}/scripts/coverage-jest.sh",
    "format": "bash ${PROJECT_ROOT}/scripts/format-prettier.sh",
    "lint": "bash ${PROJECT_ROOT}/scripts/lint-prettier.sh",
    "test": "bash ${PROJECT_ROOT}/scripts/test-jest.sh",
    "typecheck": "bash ${PROJECT_ROOT}/scripts/typecheck-tsc.sh"
  }
}
